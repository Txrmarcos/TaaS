type VerdictResult = 
 variant {
   False;
   True;
   Unknown;
 };
type Verdict = 
 record {
   confidence: float64;
   hash: text;
   resultado: VerdictResult;
   source: text;
   timestamp: Time;
 };
type Time = int;
service : {
  greet: (text) -> (text) query;
  setGreeting: (text) -> ();
  verifyStatement: (text) -> (Verdict) query;
}
