type VerdictResult = 
 variant {
   False;
   True;
   Unknown;
 };
type Verdict = 
 record {
   confidence: float64;
   hash: text;
   result: VerdictResult;
   source: text;
   timestamp: Time;
 };
type Time = int;
service : {
  getVerdictByHash: (text) -> (opt Verdict) query;
  verifyStatement: (text) -> (Verdict);
}
